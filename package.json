{
  "name": "learning-gql",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:m3lnic/learning-gql.git",
  "author": "Melody Nicholls <melody.astrid.nicholls@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn link-module-alias && yarn migrate:live && node index.js",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:live": "sequelize db:migrate",
    "createMigration": "sequelize migration:create --name"
  },
  "_moduleAliases": {
    "Models": "src/models",
    "Resolvers": "src/resolvers",
    "Helpers": "src/helpers"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@graphql-tools/utils": "^8.2.2",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "graphql": "^15.5.2",
    "graphql-tools": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "link-module-alias": "^1.2.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
